{
  "$schema": "https://raw.githubusercontent.com/nix-community/nixd/main/nixd/docs/nixd-schema.json",
  "nixpkgs": {
    "expr": "import (builtins.getFlake (toString ./.)).inputs.nixpkgs { }"
  },
  "formatting": {
    "command": "nixfmt"
  },
  "options": {
    "nixos": {
      "expr": "(builtins.getFlake (toString ./.)).nixosConfigurations.nixos-inspiron7405.options"
    },
    "home-manager": {
      "expr": "(builtins.getFlake (toString ./.)).homeConfigurations.\"weathercold@nixos-inspiron7405\".options"
    }
  }
}
